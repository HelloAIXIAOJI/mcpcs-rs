{
  "mcpServers": {
    "sse-test": {
      "transport": "sse",
      "url": "http://localhost:12121/sse"
    },
    "sse-with-auth": {
      "transport": "sse",
      "url": "http://localhost:12121/sse",
      "auth_token": "your-bearer-token-here",
      "headers": {
        "X-API-Key": "your-api-key",
        "X-Client-Version": "1.0.0"
      }
    },
    "local-test-new": {
      "transport": "child-process",
      "command": "node",
      "args": ["test-server.js"],
      "env": {}
    },
    "legacy-server": {
      "command": "python",
      "args": ["server.py"],
      "env": {
        "PYTHONPATH": "/custom/path"
      }
    }
  }
}
